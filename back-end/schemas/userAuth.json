{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "username",
        "password"
    ],
    "properties": {
        "username": {
            "type": "string",
            "title": "The username Schema",
            "minLength": "8",
            "maxLength": "20",
            "pattern": "^([a-zA-Z0-9])*$"
        },
        "password": {
            "type": "string",
            "title": "The password Schema",
            "minLength": "8",
            "maxLength": "20",
            "pattern": "^([a-zA-Z0-9])*$"
        }
    },
    "examples": [{
        "username": "username",
        "password": "ABCabc1234567890"
    }]
}
