{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "firstName",
        "lastName",
        "email",
        "password"
    ],
    "properties": {
        "firstName": {
            "type": "string",
            "default": "",
            "title": "The firstName Schema"
        },
        "lastName": {
            "type": "string",
            "default": "",
            "title": "The lastName Schema"
        },
        "email": {
            "type": "string",
            "default": "",
            "title": "The email Schema"
        },
        "password": {
            "type": "string",
            "default": "",
            "title": "The password Schema"
        },
        "isAdmin": {
            "type": "boolean",
            "default": false,
            "title": "The isAdmin Schema"
        }
    }
}